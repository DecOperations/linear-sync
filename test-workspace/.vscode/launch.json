{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension with Test Workspace",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/..",
        "${workspaceFolder}"
      ],
      "outFiles": ["${workspaceFolder}/../out/**/*.js"],
      "preLaunchTask": "${defaultBuildTask}"
    }
  ]
}
